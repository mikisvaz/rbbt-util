rbbt=/$PWD filter="*.rb *.rake Rakefile *.rdoc *.R *.sh *.js *.haml *.sass *.txt *.conf" {
 Rakefile
 bin=bin {
  excel2tsv.rb
  tsv.rb
  tchash.rb
  workflow.rb
  rbbt_query.rb
 }
 lib=lib{
  rbbt.rb
  rbbt-util.rb
  util=rbbt/util {
   resource.rb
   task.rb
   task=task{
    job.rb
   }
   workflow.rb
   workflow=workflow{
    soap.rb
   }

   simpleDSL.rb
   simpleopt.rb
   cachehelper.rb

   misc.rb
   log.rb
   tmpfile.rb
   cmd.rb
   filecache.rb
   tc_hash.rb
   open.rb
   fix_width_table.rb
   persistence.rb

   tsv.rb
   tsv=tsv{
    parse.rb
    accessor.rb
    manipulate.rb
    index.rb
    attach.rb
    resource.rb
    misc.rb
   }
   excel2tsv.rb
   bed.rb

   R.rb

   rake.rb
  }
 }
 test=test {
  test_helper.rb
  test_rbbt.rb
  test_pkg.rb
  util=rbbt/util{
   test_R.rb
   test_bed.rb
   test_cmd.rb
   test_excel2tsv.rb
   test_filecache.rb
   test_fix_width_table.rb
   test_misc.rb
   test_open.rb
   test_persistence.rb
   test_rake.rb
   test_resource.rb
   test_simpleDSL.rb
   test_simpleopt.rb
   test_tc_hash.rb
   test_tmpfile.rb
   test_tsv.rb
  }
 }
 integration_test=integration_test{
  data=data{
   genes.txt
   Metastasis.tsv
  }
  test_bed.rb
  test_namespaces.rb
  test_workflow.rb
 }
 share=share{
  Rlib=lib/R{
   util.R
  }
  software=install/software{
   lib=lib{
    install_helpers
   }
  }
 }
}
