ruby=/$PWD filter="*.rb *.rake Rakefile *.rdoc *.R *.sh *.js *.haml *.sass *.txt *.conf" {
 Rakefile
 bin=bin {
  excel2tsv.rb
 }
 lib=lib{
  rbbt.rb
  rbbt-util.rb
  util=rbbt/util {
   pkg_config.rb
   pkg_data.rb

   simpleDSL.rb
   simpleopt.rb
   cachehelper.rb

   misc.rb
   log.rb
   tmpfile.rb
   filecache.rb
   cmd.rb
   open.rb
   tc_hash.rb
   tsv.rb
  }
 }
 test=test {
  test_helper.rb
  util=rbbt/util{
   test_simpleDSL.rb

   test_misc.rb
   test_tmpfile.rb
   test_filecache.rb
   test_cmd.rb
   test_open.rb
   test_tc_hash.rb
   test_tsv.rb
  }
 }
 data=data{
 }
}
