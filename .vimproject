rbbt=/$PWD filter="*.rb *.rake Rakefile *.rdoc *.R *.sh *.js *.haml *.sass *.txt *.conf" {
 Rakefile
 bin=bin {
  excel2tsv.rb
  tsv.rb
  tchash.rb
 }
 lib=lib{
  rbbt.rb
  rbbt-util.rb
  util=rbbt/util {
   pkg_config.rb
   pkg_data.rb
   pkg_software.rb
   data_module.rb

   simpleDSL.rb
   simpleopt.rb
   cachehelper.rb

   misc.rb
   log.rb
   tmpfile.rb
   filecache.rb
   cmd.rb
   open.rb
   tc_hash.rb
   tsv.rb
   excel2tsv.rb

   rake_helper.rb
  }
 }
 test=test {
  test_helper.rb
  test_rbbt.rb
  test_pkg.rb
  util=rbbt/util{
   test_cmd.rb
   test_data_module.rb
   test_excel2tsv.rb
   test_filecache.rb
   test_misc.rb
   test_open.rb
   test_simpleDSL.rb
   test_simpleopt.rb
   test_tc_hash.rb
   test_tmpfile.rb
   test_tsv.rb
  }
 }
 share=share{
  software=install/software{
   lib=lib{
    install_helpers
   }
  }
 }
}
