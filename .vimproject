rbbt-util=/$PWD filter="*.rb *.rake Rakefile *.rdoc *.R *.sh *.js *.haml *.sass *.txt *.conf LICENSE" {
 Rakefile
 LICENSE
 README.rdoc
 bin=bin {
  excel2tsv.rb
  tsv.rb
  tchash.rb
  run_workflow.rb
  rbbt_query.rb
  rbbt_exec.rb
  rbbt_Rutil.rb
 }
 lib=lib{
  rbbt.rb
  rbbt-util.rb
  rbbt=rbbt{
   util=util{
    misc.rb
    cmd.rb
    log.rb
    open.rb

    simpleopt.rb
    simpleDSL.rb

    chain_methods.rb

    tmpfile.rb
    filecache.rb

    R.rb

    excel2tsv.rb

    rake.rb
   }

   workflow.rb
   workflow=workflow{
    accessor.rb
    annotate.rb
    step.rb
    task.rb
    soap.rb
    rest.rb
   }

   tsv.rb
   tsv=tsv{
    accessor.rb
    util.rb
    serializers.rb
    parser.rb
    manipulate.rb
    index.rb
    attach.rb
    attach=attach{
     util.rb
    }
    filter.rb
   }
   fix_width_table.rb

   persist.rb
   persist=persist{
    tsv.rb
   }

   resource.rb
   resource=resource{
    path.rb
    util.rb
    with_key.rb
    rake.rb
   }

   annotations.rb
  }
 }
 test=test {
  test_helper.rb
  test_rbbt.rb
  rbbt=rbbt{
   test_fix_width_table.rb
   test_resource.rb
   test_tsv.rb
   util=util{
    test_R.rb
    test_cmd.rb
    test_excel2tsv.rb
    test_filecache.rb
    test_misc.rb
    test_open.rb
    test_simpleDSL.rb
    test_simpleopt.rb
    test_tmpfile.rb
   }
  }
 }
 integration_test=integration_test{
  data=data{
   genes.txt
   Metastasis.tsv
  }
  test_bed.rb
  test_namespaces.rb
  test_workflow.rb
 }
 share=share{
  Rlib=lib/R{
   util.R
  }
  software=install/software{
   lib=lib{
    install_helpers
   }
  }
 }
}
