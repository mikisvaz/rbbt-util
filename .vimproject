rbbt=/$PWD filter="*.rb *.rake Rakefile *.rdoc *.R *.sh *.js *.haml *.sass *.txt *.conf" {
 Rakefile
 bin=bin {
  excel2tsv.rb
  tsv.rb
  tchash.rb
 }
 lib=lib{
  rbbt.rb
  rbbt-util.rb
  util=rbbt/util {
   pkg_config.rb
   pkg_data.rb
   pkg_software.rb
   data_module.rb

   simpleDSL.rb
   simpleopt.rb
   cachehelper.rb

   misc.rb
   path.rb
   namespace.rb
   log.rb
   tmpfile.rb
   cmd.rb
   filecache.rb
   open.rb
   tc_hash.rb
   fix_width_table.rb
   persistence.rb

   tsv.rb
   tsv=tsv{
    parse.rb
    accessor.rb
    manipulate.rb
    index.rb
    attach.rb
   }
   excel2tsv.rb
   bed.rb

   R.rb

   rake.rb

   workflow.rb
  }
 }
 test=test {
  test_helper.rb
  test_rbbt.rb
  test_pkg.rb
  util=rbbt/util{
   test_R.rb
   test_bed.rb
   test_cmd.rb
   test_data_module.rb
   test_excel2tsv.rb
   test_filecache.rb
   test_misc.rb
   test_open.rb
   test_pkg_data.rb
   test_rake.rb
   test_simpleDSL.rb
   test_simpleopt.rb
   test_tc_hash.rb
   test_tmpfile.rb
   test_tsv.rb
   test_workflow.rb
  }
 }
 integration_test=integration_test{
  data=data{
   genes.txt
   Metastasis.tsv
  }
  test_bed.rb
  test_workflow.rb
 }
 share=share{
  Rlib=lib/R{
   util.R
  }
  software=install/software{
   lib=lib{
   }
  }
 }
}
